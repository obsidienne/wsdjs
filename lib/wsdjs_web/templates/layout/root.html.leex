      <%= link "Events", to: Routes.event_path(@conn, :index), class: "glnav__menu hidden md:inline #{active_class(@conn, ["event"])}" %>
      <% end %>

      <%= if @current_user do %>
      <%= link to: Routes.user_path(@conn, :show, @current_user.id), class: "glnav__user" do %>
      <img class="glnav__user__avatar" src="<%= Attachments.avatar_url(@current_user.avatar, 40) %>"
