{
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": true,
    "window.zoomLevel": 0,
    "editor.renderWhitespace": "all",
    "editor.formatOnSave": true,
    "editor.rulers": [
        80,
        120
    ],
    "files.associations": {
        "*.html.eex": "html-eex"
    },
    "emmet.includeLanguages": {
        "html-eex": "html"
    }
}