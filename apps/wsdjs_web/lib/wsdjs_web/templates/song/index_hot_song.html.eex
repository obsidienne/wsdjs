<%= if @last == false do %>
<section class="container" data-month="<%= @month %>">
    <%= if Enum.count(@songs) > 0 do %>
    <h4 class="page-header">
        <time datetime="<%= @month %>"></time> <small><%= Enum.count(@songs) %> suggestions</small>
    </h4>
    <ul class="hot-songs grid">
        <%= render_many @songs, WsdjsWeb.SharedView, "_hot_song.html", conn: @conn, as: :song, current_user: @current_user %>
    </ul>
    <% end %>

    <div class="sentinel"></div>
</section>
<% end %>
