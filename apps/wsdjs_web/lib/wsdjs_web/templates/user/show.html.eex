.popup.mini-profile.top-left::before {
    top: -8px;
    transform: rotate(45deg)
}

.popup.mini-profile.bottom-right::before,
.popup.mini-profile.bottom-left::before {
    bottom: -8px;
    transform: rotate(-134deg)
}

.popup.mini-profile.top-right::before,
