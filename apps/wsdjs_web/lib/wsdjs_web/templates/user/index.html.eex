<%= render WsdjsWeb.SharedView, "_nav.html", conn: @conn, current_user: @current_user %>

<main class="container">
  <h1>Listing Users</h1>

  <table class="table">
  <%= for user <- @users do %>
  <tr>
    <td>
    <img src="<%= avatar_url(user.avatar) %>" width="30px" height="30px"/><b>
    <%= link user.email, to: user_path(@conn, :show, user.id) %>
    </td>
  </tr>
  <% end %>
  </table>
</main>
